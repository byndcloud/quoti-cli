{
  "name": "quoti-cli",
  "description": "A Quoti Extensions CLI",
  "version": "0.3.0",
  "author": "Nathan @nmf2",
  "preferGlobal": true,
  "bin": {
    "qt": "./bin/run"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.7.2",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^5.1.10",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.1",
    "dotenv": "^8.2.0",
    "firebase": "^8.10.0",
    "fuzzy": "^0.1.3",
    "get-dependency-tree": "^4.0.1",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "inquirer-file-tree-selection-prompt": "^1.0.13",
    "json-file-plus": "^3.3.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "ora": "^5.4.1",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.3.5",
    "vue-cli-plugin-pug": "^2.0.0",
    "vue-template-compiler": "^2.6.12",
    "winston": "^3.3.3",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@sinonjs/referee": "^9.1.1",
    "findup-sync": "^5.0.0",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.11",
    "@types/md5": "^2.3.0",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.5.1",
    "proxyquire": "^2.1.3",
    "sinon": "^13.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/nmf2/cliv2",
  "keywords": [
    "cli-quoti",
    "quoti",
    "cli",
    "qt"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "qt",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "prerun": "./src/hooks/login",
      "init": "./src/hooks/node-version"
    }
  },
  "repository": "https://github.com/byndcloud/quoti-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test:unit": "nyc mocha --forbid-only --exit \"test/unit/**/*.test.js\"",
    "test:endToEnd": "nyc mocha --forbid-only --exit \"test/end-to-end/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "quoti": {
    "extensions": [
      "extension/src/App.vue",
      "test/extensionToTest/App.vue"
    ]
  }
}
