{
  "name": "quoti-cli",
  "description": "A Quoti Extensions CLI",
  "version": "0.6.0",
  "author": "Nathan @nmf2",
  "preferGlobal": true,
  "bin": {
    "qt": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-help": "^5.1.10",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.1",
    "dotenv": "^8.2.0",
    "firebase": "^8.10.1",
    "fuzzy": "^0.1.3",
    "get-dependency-tree": "^4.0.1",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "inquirer-file-tree-selection-prompt": "^1.0.13",
    "json-file-plus": "^3.3.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "ora": "^5.4.1",
    "qs": "^6.10.3",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.3.5",
    "unzipper": "^0.10.11",
    "vue-cli-plugin-pug": "^2.0.0",
    "vue-template-compiler": "^2.6.12",
    "winston": "^3.3.3",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@sinonjs/referee": "^9.1.1",
    "@types/chai": "^4.2.15",
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.11",
    "@types/md5": "^2.3.0",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "findup-sync": "^5.0.0",
    "globby": "^10.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.5.1",
    "proxyquire": "^2.1.3",
    "sinon": "^13.0.1"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/nmf2/cliv2",
  "keywords": [
    "cli-quoti",
    "quoti",
    "cli",
    "qt"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "qt",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "hooks": {
      "prerun": [
        "./src/hooks/login",
        "./src/hooks/show-info-on-localhost"
      ]
    }
  },
  "repository": "https://github.com/byndcloud/quoti-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test:unit": "NODE_ENV=test nyc mocha --forbid-only --exit \"test/unit/**/*.test.js\"",
    "test:e2e": "NODE_ENV=test nyc mocha --forbid-only --exit \"test/end-to-end/**/*.test.js\"",
    "generate:docs": "npm pack && git add README.md",
    "lint": "eslint --fix -c .eslintrc.json src/**/*.js test/**/*.js",
    "format": "prettier --write src/**/*.js test/**/*.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
