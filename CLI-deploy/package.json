{
  "author": "Nathan @nmf2",
  "name": "quoti-cli",
  "version": "1.2.10",
  "preferGlobal": true,
  "bin": {
    "qt": "./bin/run"
  },
  "dependencies": {
    "7zip-min": "^1.4.3"
  },
  "description": "A Quoti Extensions CLI",
  "engines": {
    "node": ">=14.21.3"
  },
  "files": [
    "bin",
    "index.js",
    "platforms",
    "README.md"
  ],
  "keywords": [
    "cli-quoti",
    "quoti",
    "cli",
    "qt"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "qt",
    "commands": "./src/commands",
    "hooks": {
      "prerun": [
        "./src/hooks/login",
        "./src/hooks/show-info-on-localhost"
      ]
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "https://github.com/byndcloud/quoti-cli",
  "scripts": {
    "postinstall": "node index.js"
  }
}
